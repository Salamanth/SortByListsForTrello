{
  "manifest_version": 2,

  "name": "Trello Sort Cards by Steps",
  "description": "Organizes cards by custom steps in Trello",
  "version": "0.1",
  "icons": { "128": "icon.png" },

  "options_page": "options.html",

  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "js": ["jquery-1.11.3.min.js", "script.js"]
    }
  ],

  "permissions": [
    "activeTab"
  ]
}
